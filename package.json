{
  "name": "codenames",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test": "vue-cli-service e2e --record",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "flask": "venv/bin/python server/app.py",
    "gunicorn": "venv/bin/gunicorn --error-logfile - --worker-class eventlet -w 1 server:app -b 0.0.0.0:5000",
    "setup": "venv/bin/pip install -r requirements.txt && npm install",
    "e2e:open": "vue-cli-service e2e:open",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "js-cookie": "^2.2.0",
    "raivue": "^0.1.9",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-resize-text": "^0.1.1",
    "vue-router": "^3.1.6",
    "vue-socket.io": "^3.0.10",
    "vuetify": "^2.2.26",
    "vuex": "^3.3.0",
    "vuex-persistedstate": "^2.7.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
